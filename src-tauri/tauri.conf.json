{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173"
  },
  "plugins": {
    "sql": {
      "preload": ["sqlite:meadtools.db"]
    },
    "deep-link": {
      "desktop": {
        "schemes": ["meadtools"]
      }
    },
    "updater": {
      "endpoints": [
        "https://cdn.crabnebula.app/update/meadtools/meadtools/{{target}}-{{arch}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDk2RjIwQ0E5NzkwRDU4NUYKUldSZldBMTVxUXp5bGpreFdOQnRsdi9ySks2R29GcHkxcWNsWmFodEo5VGdhSDRyS0MrZk9iVkEK",
      "dangerousInsecureTransportProtocol": true
    }
  },
  "bundle": {
    "createUpdaterArtifacts": true,
    "active": true,
    "category": "DeveloperTool",
    "copyright": "",
    "targets": "all",
    "externalBin": [],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "longDescription": "",
    "macOS": {
      "entitlements": null,
      "exceptionDomain": "",
      "frameworks": [],
      "providerShortName": null,
      "signingIdentity": null
    },
    "resources": [],
    "shortDescription": "",
    "linux": {
      "deb": {
        "depends": []
      }
    }
  },
  "productName": "MeadTools",
  "mainBinaryName": "MeadTools",
  "version": "1.0.1",
  "identifier": "meadtools",
  "app": {
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "MeadTools",
        "width": 800
      }
    ],
    "security": {
      "csp": null
    }
  }
}
